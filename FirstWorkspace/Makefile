.PHONY: clean All

All:
	@echo "----------Building project:[ GuessTheNumber - Debug ]----------"
	@cd "GuessTheNumber" && "$(MAKE)" -f  "GuessTheNumber.mk"
clean:
	@echo "----------Cleaning project:[ GuessTheNumber - Debug ]----------"
	@cd "GuessTheNumber" && "$(MAKE)" -f  "GuessTheNumber.mk" clean
